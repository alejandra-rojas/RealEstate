{
  "routes": [
    {
      "route": "/dashboard",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/files/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "serve": "/index.html",
      "statusCode": 200
    }
  }
}
